<?xml version="1.0" encoding="UTF-8"?>
<constraints>
  <hostlabel>reference_benchmark_host</hostlabel>
  <sandbox>secure</sandbox>
  <linux>
    <version>
      <max>3.42</max>
      <min>1.0</min>
    </version>
    <flavor>default</flavor>
  </linux>
  <hardware>
    <cpu>
      <flag>fpu</flag>
      <flag>mmx</flag>
    </cpu>
    <processors>2</processors>
    <disk>
      <size unit="T">2</size>
    </disk>
    <memory>
      <size unit="G">1.5</size>
    </memory>
    <physicalmemory>
      <size unit="M">512</size>
    </physicalmemory>
  </hardware>
</constraints>
