# On qemu arches, kernel-obs-build does not make any sense
# on ARMv7 you can not boot the kernel as a guest, there is a special
# guest kernel.
%ifnarch armv6l armv6hl armv7l armv7hl aarch64
VMinstall: kernel-obs-build
%endif

# Need to copy 64bit kernel for 32bit builds, enabled this after 
# release up-on request from Dinar to fix hanging builds with new workers
ExportFilter: ^kernel-obs-build.*\.ppc64\.rpm$ . ppc

%ifarch aarch64 armv6l armv6hl
%if &quot;%_repository&quot; != &quot;native&quot;
%define qemu_user_space_build 1
%endif
%endif

