- @pagetitle = 'Sign Up'

.card.mb-3
  = render partial: 'webui/configuration/tabs'
  .card-body
    .col-lg-6.pl-0
      - if can_register
        %h3= @pagetitle
        = render partial: 'webui2/shared/sign_up', locals: { submit_btn_text: 'Sign Up' }
      - else
        %p Sorry, sign up is disabled
